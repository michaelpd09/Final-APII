@model IEnumerable<SistemaInventario.Models.TiposUsuarios>

@{
    ViewBag.Title = "Registro de Tipos de Usuarios";
}

<h2>Registro de Tipos de Usuarios</h2>

<p>
    @Html.ActionLink("Crear Nuevo", "Create", "TiposUsuarios", null, new { @class = "btn btn-primary btn-large" })
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fecha)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fecha)
        </td>
        <td>
           
          
            @Html.ActionLink("Editar", "Edit", new { id=item.TiposUsuariosId }) |
            @Html.ActionLink("Detalle", "Details", new { id=item.TiposUsuariosId }) |
            @Html.ActionLink("Borrar", "Delete", new { id=item.TiposUsuariosId })
        </td>
    </tr>
}

</table>
