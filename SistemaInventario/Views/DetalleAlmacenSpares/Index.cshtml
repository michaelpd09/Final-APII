@model IEnumerable<SistemaInventario.Models.DetalleAlmacenSpares>

@{
    ViewBag.Title = "Dellate Spare Almacen";
}

<h2>Dellate Spare Almacen</h2>

<p>
    @Html.ActionLink("Crear Nuevo", "Create", "DetalleAlmacenSpares", null, new { @class = "btn btn-primary btn-large" })
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.RegistroAlmacen.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RegistroSpares.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Tecnologia)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fecha)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.RegistroAlmacen.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RegistroSpares.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Tecnologia)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fecha)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.DetalleAlmacenSpareId }) |
            @Html.ActionLink("Detalle", "Details", new { id=item.DetalleAlmacenSpareId }) |
            @Html.ActionLink("Borrar", "Delete", new { id=item.DetalleAlmacenSpareId })
        </td>
    </tr>
}

</table>
